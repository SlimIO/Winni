{
  "name": "@slimio/winni",
  "version": "1.1.0",
  "description": "Windows Network Interfaces (winni)",
  "main": "winni.js",
  "engines": {
    "node": ">=10"
  },
  "husky": {
    "hooks": {
      "pre-push": "cross-env npm test && eslint index.js",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 98,
    "statements": 98,
    "functions": 98,
    "branches": 90
  },
  "scripts": {
    "prepublishOnly": "pkg-ok",
    "test": "ava --verbose",
    "prebuilds": "prebuildify --napi",
    "build": "cross-env node-gyp configure && node-gyp build",
    "install": "node-gyp rebuild",
    "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
    "coverage": "nyc ava",
    "report": "nyc report --reporter=html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlimIO/winni.git"
  },
  "keywords": [
    "SlimIO",
    "Monitoring",
    "Binding",
    "NAPI",
    "Interfaces",
    "Network",
    "Microsoft",
    "Windows",
    "IfEntry",
    "IfTable",
    "IfRow"
  ],
  "author": "SlimIO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SlimIO/winni/issues"
  },
  "homepage": "https://github.com/SlimIO/winni#readme",
  "dependencies": {
    "node-addon-api": "^1.6.3",
    "node-gyp-build": "^3.8.0"
  },
  "private": false,
  "gypfile": true,
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@escommunity/minami": "^1.0.0",
    "@slimio/eslint-config": "^2.0.4",
    "@slimio/is": "^1.5.0",
    "ava": "^1.4.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "husky": "^2.1.0",
    "jsdoc": "^3.5.5",
    "node-gyp": "^3.8.0",
    "nyc": "^13.3.0",
    "pkg-ok": "^2.3.1",
    "prebuildify": "^2.11.0"
  }
}
